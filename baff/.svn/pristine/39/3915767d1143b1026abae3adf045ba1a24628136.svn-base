var util = require('../../../utils/util.js')
var app = getApp()
Page({
  data:{
    scrollHeight:0,
    detail_list:[],
  },

  onLoad:function(){
    var that=this
    this.getDetailList()
    console.log(this.data.detail_list)
    wx.getSystemInfo({
      success: function (res) {
        that.setData({
          scrollHeight: res.windowHeight
        });
      }
    });
  },

  //获取专题列表数据
  getDetailList: function () {
    var detail_list = util.getDetailList()
    console.log("获取专题列表数据")
    this.setData({
      detail_list: detail_list
    });
  },
})