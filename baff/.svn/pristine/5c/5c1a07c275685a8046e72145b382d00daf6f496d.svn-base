var util = require('../../../utils/util.js')
var app = getApp()
Page({
  data:{
    current:0,
    label:[],
  },

  onLoad:function(){
    this.getLabel()
    console.log(this.data.label)
  },

  forward:function(){
    var current=this.data.current
    if(current == 0){
      return
    }else{
      current=current-1
      this.setData({
        current:current
      })
    }

  },

  back: function () {
    var current = this.data.current
    if (current == (this.data.label.length-1)) {
      return
    } else {
      current = current + 1
      this.setData({
        current: current
      })
    }

  },


  //获取名词标记数据
  getLabel: function () {
    var label = util.getLabel()
    console.log("获取tips数据")
    for(var i in label){
        label[i].index = '第' + (parseInt(i) + 1) + '页'
    }
    this.setData({
      label: label
    })
  },
})