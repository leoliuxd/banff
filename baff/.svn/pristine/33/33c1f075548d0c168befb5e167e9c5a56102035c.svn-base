<view class="Page">
  <scroll-view scroll-y="true" style="height:{{scrollHeight}}px;">
    <block wx:for="{{text}}" wx:key="*this">
      <view class="content_item">
        <view class="title">{{item.title}}</view>
        <view class="detail">{{item.detail}}</view>
      </view>
    </block>
  </scroll-view>
</view>