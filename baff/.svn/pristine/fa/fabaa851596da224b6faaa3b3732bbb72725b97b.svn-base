<view class="Page">
  <scroll-view scroll-y="true" class="list" style="height:{{scrollHeight}}px;">
    <block wx:for="{{detail_list}}" wx:key="id">
      <navigator url="{{item.url}}" class="item">
          <image class="poster" src="{{item.img}}"></image>
          <view class="title">-- {{item.name}} --</view>
      </navigator>
    </block>
  </scroll-view>
</view>