var util=require('../../../utils/util.js')
var app = getApp()
Page({
    data: {
        list: [],
        scenery:{},
        advention:{},
    },

    onLoad:function(){
        console.log('onLoad') 
        this.getAdvention()
    },

    kindToggle: function (e) {
        var id = e.currentTarget.id
        this.data[id].open = !this.data[id].open 
        this.setData({
          [id]: this.data[id]
         });
    },

    //获取探险数据
    getAdvention: function(){
    var list = util.getAdvention()
    console.log("获取探险数据")
    //console.log(list)
    this.setData({
      list:list,
      scenery:list[0],
      advention:list[1]
    });
  },
});
