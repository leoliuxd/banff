.list{
    display: flex;
    flex: 1;
    flex-direction: column;
}

.list .swiper{
  width:100%;
  height:538rpx; 
}

.list .swiper .img{
  width:100%;
  height: 100%;
}

.meta{
    display: flex;
    flex: 1;
    flex-direction: column;
}

.meta .name{
  font-size: 44rpx;
  color: rgb(104, 46, 107);
  margin: 52rpx;
  font-weight: bold;
}

.meta .profile{
  font-size: 28rpx;
  line-height: 50rpx;
  font-weight: bold;
  margin: 3rpx 60rpx 65rpx;
}

.details{
  width:100%;
  height: 321rpx;
}

.details .edge1{
  width:25rpx;
  float:left;
  position: relative;
  height: 100%;
}

.details .edge1 .angle1{
  width:100%;
  height:64rpx;
  position: absolute;
  top:257rpx;
  left:0rpx;
  background-color: black;
}

.details .edge2{
  width:80rpx;
  float:left;
  position: relative;
  height: 100%;
  background-color: rgb(103, 46, 108)
}

.details .edge2 .angle2{
  width:18rpx;
  height:86rpx;
  position: absolute;
  top:0rpx;
  left:34rpx;
  background-color: rgb(253, 183, 23);
}


.details .content{
    display: flex;
    flex: 1;
    flex-direction: column;
    float:left;
    height: 100%;
    width:646rpx;
    background-color: rgb(103, 46, 108)
}

.details .content .address{
  width: 100%;
}

.details .content .address .img{
  width:31rpx;
  height: 34rpx;
  float: left;
  margin-left: 9rpx;
  margin-top: 48rpx;
}

.details .content .address .text{
  font-size: 25rpx;
  margin-left: 19rpx;
  color: white;
  float: left;
  margin-top: 48rpx;
  font-weight: bold;
  margin-right: 25rpx;
}

.details .content .telphone .img{
  width:32rpx;
  height: 34rpx;
  float: left;
  margin-left: 9rpx;
  margin-top: 28rpx;
}

.details .content .telphone .text{
  font-size: 28rpx;
  margin-left: 19rpx;
  color: white;
  float: left;
  margin-top: 26rpx;
  font-weight: bold;
  margin-right: 25rpx;
}

.details .content .email .img{
  width:34rpx;
  height: 34rpx;
  float: left;
  margin-left: 1rpx;
  margin-top: 28rpx;
}

.details .content .email .text{
  font-size: 28rpx;
  margin-left: 13rpx;
  color: white;
  float: left;
  margin-top: 26rpx;
  font-weight: bold;
  margin-right: 25rpx;
}

.details .content .website .img{
  width:34rpx;
  height: 34rpx;
  float: left;
  margin-left: 2rpx;
  margin-top: 26rpx;
}

.details .content .website .text{
  font-size: 28rpx;
  margin-left: 20rpx;
  color: white;
  float: left;
  margin-top: 24rpx;
  font-weight: bold;
  margin-right: 25rpx;
}



.space{
  height: 150rpx;
  width: 100%;
  background-color: black;
  border-left: 1rpx solid white;
}