var util = require('../../../utils/util.js')
var app = getApp()
Page({
    data:{
      scrollHeight:0,
      title:[],
    },

    onLoad:function(){
      var that=this
      this.getTips()
      wx.getSystemInfo({
        success: function (res) {
          that.setData({
            scrollHeight: res.windowHeight
          });
        }
      });
    },


    //获取tips数据
    getTips: function () {
      var tips = util.getTips()
      var title=this.data.title
      console.log("获取tips数据")
      for(var i in tips){
        title.push(tips[i].name)
      }
      this.setData({
        title:title
      })
    },




})